<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>marin.manuel@neurobio.org</em:id>
		<em:name>Pubpeer Plugin for Zotero</em:name>
		<em:version>0.0.0</em:version>
		<em:description>Plugin to show if references in Zotero have PubPeer comments </em:description>
		<em:creator>Marin MANUEL</em:creator>
		<em:updateURL></em:updateURL>
		<em:type>2</em:type> <!-- type=extension -->
		
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>5.0</em:minVersion>
				<em:maxVersion>5.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
